{
  "env": {
    "browser": true,
    "node": true,
      "es6": true
  },
  "extends": [
    "@antfu",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error",
    "camelcase": "off",
    "vue/no-multiple-template-root": "off",
    "vue/multi-word-component-names": "off"
  }
}